<!DOCTYPE html>
<html lang="pt">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, minimal-ui">
        <title>{% block title %}{% endblock %} | Consola de Gest√£o</title>
        {% image '@SalesEngineOnlineDesafioBundle/Resources/public/images/favicon.ico' %}
        <link rel="shortcut icon" href="{{ asset_url }}" />
        {% endimage %}
        {% stylesheets 
            '@SalesEngineOnlineDesafioBundle/Resources/public/css/bootstrap.min.css'
        %}
        <link rel="stylesheet" type="text/css" href="{{ asset_url }}">
        {% endstylesheets %}
    {% block styles %}{% endblock %}
</head>
<body>
    <div id="wrapper">
        <div class="container-fluid">
            <div class="content">
                <div class="row">
                    <div id="header">
                        {% include 'SalesEngineOnlineDesafioBundle:template:menu.html.twig' %}
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid">
            <div class="content">
                <div class="row">
                {% block content %}{% endblock %}
                </div>
            </div>
        </div>
    </div>

<!-- Javascript -->
{% javascripts 
                    '@SalesEngineOnlineDesafioBundle/Resources/public/js/jquery-1.11.0.min.js' 
                    '@SalesEngineOnlineDesafioBundle/Resources/public/js/bootstrap.min.js' 
                    '@SalesEngineOnlineDesafioBundle/Resources/public/js/selectforremoval.min.js' 
                    '@SalesEngineOnlineDesafioBundle/Resources/public/js/delete.min.js' 
                    '@SalesEngineOnlineDesafioBundle/Resources/public/js/hide_alerts.min.js' 
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
{% block javascripts %}{% endblock %}
<!-- Javascript -->
</body>
</html>